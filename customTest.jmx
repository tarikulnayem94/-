for (int second = 0;; second++) {
	if (second &gt;= 300 ){
        WDS.sampleResult.sampleEnd();
        WDS.sampleResult.setSuccessful(false);
        WDS.sampleResult.setResponseMessage(&quot;Failed to load page, too slow!&quot;);
        return;
    }
